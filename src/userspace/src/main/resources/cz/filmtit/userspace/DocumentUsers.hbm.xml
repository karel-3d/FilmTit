<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cz.filmtit.userspace.DocumentUsers" table="DocumentUsers">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
            
        
        <property name="userId" type="long" column="userId"/> 
        <property name="moviePath" type="string" column="moviePath"/>
        
     <!--   <one-to-one name="user" class="cz.filmtit.userspace.USUser" constrained="true"/>   -->     
    </class>
</hibernate-mapping>
